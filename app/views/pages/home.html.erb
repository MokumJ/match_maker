<h1>Hello, <%= current_user.profile.first_name %>!</h1>
<table class="table table-striped table-bordered">
	<tr>
		<th>Day</th>
		<th>Your pair for the day</th>
	</tr>
	<% @pairs_schedule.each do |day| %>
		<% if day.day <= Date.today %>
			<% day.pairs.each do |pair| %>
				<% if pair.include?(current_user.profile.full_name) %>
					<tr>
						<td><%= day.day %></td>
						<td><%= pair %></td>
					</tr>
				<% end %>
			<% end %>
		<% end %>
	<% end %>
</table>
