<h1>Show all pair combination</h1>

<table class="table table-striped table-bordered">
	<tr>
		<th>Day</th>
		<th>Pairs of the day</th>
	</tr>
	<% @pairs_schedule.each do |day| %>
	<tr>
		<td><%= day.day %></td>
		<td><%= day.pairs %></td>
	</tr>

	<% end %>

</table>

	<%= button_to "auto generate", save_into_db_path %>
