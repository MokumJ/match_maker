<h1>Show all pair combination</h1>

<table class="table table-striped table-bordered">
	<tr>
		<th>Day</th>
		<th>Pairs of the day</th>
	</tr>
	<% @pairs.each do |pair| %>
	<tr>
		<td><%= "#{pair.day}" %></td>
		<td><%= "#{pair.pairs.each {|pair| print "(" + pair.to_s + ")" }}" %></td>
	</tr>
	<% end %>
</table>
